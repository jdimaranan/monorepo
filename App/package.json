{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "copywebviewerfiles": "mkdirp ./public/webviewer/lib && cp -r ../node_modules/@pdftron/webviewer/public/* ./public/webviewer/lib",
    "copywebviewerfiles:windows": "mkdirp \"./public/webviewer/lib\" && xcopy \"../node_modules/@pdftron/webviewer/public\" \"./public/webviewer/lib\" /s /e /y"
  },
  "dependencies": {
    "common-library": "*"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
